{
  "name": "material-ui-color-picker",
  "version": "4.0.1",
  "description": "<ColorInput> component for material-ui",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint ./src/ --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint-fix": "npm run lint -- --fix",
    "build": "tsc -b",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prepare": "npm run build",
    "now-build": "build-storybook -c .storybook -o public",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "files": [
    "LICENSE",
    "*.md",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loicmahieu/material-ui-color-picker.git"
  },
  "keywords": [
    "reactjs",
    "react",
    "color",
    "picker",
    "selector"
  ],
  "author": "Loic Mahieu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loicmahieu/material-ui-color-picker/issues"
  },
  "homepage": "https://github.com/loicmahieu/material-ui-color-picker#readme",
  "devDependencies": {
    "@storybook/addon-actions": "~6.4.22",
    "@storybook/react": "~6.4.22",
    "@types/react": "~18.0.8",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "~18.0.3",
    "@typescript-eslint/eslint-plugin": "~5.21.0",
    "@typescript-eslint/parser": "~5.21.0",
    "eslint": "~8.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "~7.29.4",
    "eslint-plugin-react-hooks": "~4.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "~2.6.2",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,scss,sass}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "~11.9.0",
    "@emotion/styled": "~11.8.1",
    "@mui/material": "^5.6.3",
    "react-color": "^2.19.3",
    "react-dom": "^18.0.0",
    "react": "^18.0.0"
  }
}
